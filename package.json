{
  "name": "@abmao/forb",
  "version": "2.0.4",
  "description": "Get the directory path of `packages` and `packages.json` path",
  "type": "module",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "index.d.ts"
  ],
  "scripts": {
    "build": "tsup index.js --format esm,cjs --target node12.20.0",
    "test": "cd ./examples && node index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hengshanMWC/forb.git"
  },
  "keywords": [
    "monorepo",
    "packages"
  ],
  "author": "mawencan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hengshanMWC/forb/issues"
  },
  "homepage": "https://github.com/hengshanMWC/forb#readme",
  "dependencies": {
    "glob": "^10.2.7"
  },
  "devDependencies": {
    "tsup": "^7.0.0"
  }
}
